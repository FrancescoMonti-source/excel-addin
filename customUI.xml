<customUI xmlns="http://schemas.microsoft.com/office/2009/07/customui" onLoad="Ribbon_OnLoad">
  <ribbon>
    <tabs>
      <tab id="tabMyTools" label="My tools">
        <group id="text_tools" label="Text tools">
          <button id="btnUpper"   imageMso = "ChangeCase"                label="to_upper"                 onAction="ToUpperCase_UI"     size="large"/>
          <button id="btnLower"   imageMso = "ChangeCase"                label="to_lower"                 onAction="ToLowerCase_UI"/>
          <button id="btnDiacr"   imageMso = "ClearFormatting"           label="rm_accent"                onAction="RemoveDiacritics_UI"/>
          <button id="btnFormat"  imageMso = "FontColorCycle"            label="Format&#10;matching text" onAction="FormatMatchingText_UI"/>
          <button id="btnColorID" imageMso = "ConditionalFormattingMenu" label="ColorID"                  onAction="ColorID_UI"/>
          <button id="btnFixVals" imageMso = "PasteValues"               label="Fix Values&#10;In Place"  onAction="FixValuesInPlace_UI"/>
        </group>
        <group id="miscellanea" label="Miscellanea">
          <button id="btnGrabImg" imageMso = "PictureInsertFromFile"     label="Grab image&#10;from URL"  onAction="GrabImageFromUrl_UI"/>    
          <button id="btnTrimUR"  imageMso = "ClearAll"                  label="Trim&#10;UsedRange"       onAction="TrimAndResetUsedRange_UI"/>  
        </group>
      </tab>
    </tabs>
  </ribbon>
</customUI>
